                <div class="container-sticky">
                    <h3 class="promotion-text">
                        GET FULL ACCESS PLANS AT 25% OFF
                    </h3>
                    <img src="https://images.vexels.com/img/assets/dino.gif?v=110" alt="Vexels pricing" title="Vexels plans" class="gif-total"> 
                </div>
                <div style="float: left;display: flex; align-items: center; margin-left: 15px;margin-top: 3px;">
                    <div class="support-us-total"> 
                        <a href="/plans/bb">Buy Now</a>
                    </div>
                </div>
            </div>`;
            $('#sticky-Left-Banner').addClass('sticky-total');
        }
        $('#sticky-Left-Banner').html(stickyLeft_Banner);
        
        if(random < 0.5){
            var check_plans = $('#sticky-Left-Banner .support-us-total a').html();
            check_plans = check_plans.replace("<br>"," ");
            $('#sticky-Left-Banner .support-us-total a').html(check_plans);
        }
    });

    $('#one-time-button').click(function(){
        if(globalUserIsLogged){            
            sessionStorage.setItem("step_plans", 2);
            $('#step-2-hidden').show();
            $('#step-2-login-hidden').hide();
            $('#step-3-hidden').hide();
        }
        else{
            sessionStorage.setItem("step_plans", 3);
            $('#step-2-hidden').hide();
            $('#step-2-login-hidden').show();
            $('#step-3-hidden').show();
        }
    });

    $('#subscribe-btn-download').click(function(){
        sessionStorage.setItem("step_plans", 3);
        $('#step-2-hidden').hide();
        $('#step-2-login-hidden').show();
        $('#step-3-hidden').show();
    });
    

    $('.pay_tab_box_btns a').click(function(){
            
        if(sessionStorage.getItem("step_plans")==2){   
            $('#step-2-hidden').show();
            $('#step-2-login-hidden').hide();
            $('#step-3-hidden').hide();
        }
        else{  
            $('#step-2-hidden').hide();
            $('#step-2-login-hidden').show();
            $('#step-3-hidden').show();
        }  

        setTimeout(function(){            
            if(globalUserIsLogged){   
                sessionStorage.setItem("step_plans", 2);
            }
            else{  
                sessionStorage.setItem("step_plans", 3);
            }    
        }, 1000);
    })

    $('.pricing').click(function(){
        if(globalUserIsLogged){            
            sessionStorage.setItem("step_plans", 2);
        }
        else{
            sessionStorage.setItem("step_plans", 3);
        }
    })

    if($('#step-1-hidden').length){
        setTimeout(function(){
            if(sessionStorage.getItem("step_plans")==2){
                $('#step-2-hidden').show();
            }
            else{
                $('#step-2-login-hidden').show();
                $('#step-3-hidden').show();
            }
                
            if(globalUserIsLogged){   
                sessionStorage.setItem("step_plans", 2);
            }
            else{  
                sessionStorage.setItem("step_plans", 3);
            } 
        }, 400);
    }
</script>

