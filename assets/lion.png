function actionOnCheckLogin(){
    consoleLog("logged user is: " + globalUserLoggedId);
    if(!globalUserHasLicence && !globalUserIsSubscriptor){
        $(".content-istock").removeClass("hidden");
        makeIStockSearchSideFrame("lion+cartoon", "", 1, 35, $("#id_vector").val());
        if($("#is_editorial").val() != "yes"){ 
            $("#buy_commercial").show();
            $("#buy_commercial_prices").show();
        } else{
            $("#buy_commercial_text").html("This graphic features trademarks, logos, and/or celebrities. It can only be used for editorial purposes unless you are an authorized reseller. Learn more on our <a target='_blank' href='https://www.vexels.com/terms-and-conditions/'>Terms & Conditions.</a>");
            $("#buy_commercial").css("font-size", "14px;");
            $("#buy_commercial").show();
        }
    } else{
        if($("#is_editorial").val() == "yes"){ 
            $("#buy_commercial_text").html("This graphic features trademarks, logos, and/or celebrities. It can only be used for editorial purposes unless you are an authorized reseller. Learn more on our <a target='_blank' href='https://www.vexels.com/terms-and-conditions/'>Terms & Conditions.</a>");
            $("#buy_commercial").css("font-size", "14px;");
            $("#buy_commercial").show();
            $(".content-istock").addClass("hidden");
        } 
    }

    if(globalUserLoggedId == 3 || globalUserLoggedId == 29873 || globalUserLoggedId == 59133){
        $("#downloads_count").show();
    }
}

function checkLicence(){
    consoleLog("Running checkLicence");

    if(globalUserStatusLoaded){
        if(globalUserHasLicence){
            consoleLog("has licence!");
            if(globalUserIsSubscriptor){
                consoleLog("is subscriptor!");
                $("#subs_purchase_block").hide();
            } else{
                consoleLog("is NOT subscriptor!");
                $("#subs_purchase_block").show();
            }
            userhasLicence = "yes";
            $("#purchase_block").hide();
        } else{
            consoleLog("has not licence!");
            $("#purchase_block").show();
            $("#subs_purchase_block").hide();
            userhasLicence = "no"; 
        }
        bindEvents();
        //common_debug_console = false;
    } else{
        consoleLog("waiting for checkLicence");
        setTimeout(checkLicence, loopInterval);
    }
}

